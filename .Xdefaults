!!!!!! XConfig file !!!!!!

!!! URxvt-unicode !!!
! Terminal
URxvt*termName: rxvt-256color
! Transparency
!urxvt*transparent : true
!urxvt*shading : 5
! Font
urxvt*font: xft:mono:size=10	
! urxvt*boldFont: xft:mono:bold:size=8:antialias=false
! urxvt*italicFont: xft:mono:italic:size=8:antialias=false
! urxvt*boldItalicFont: xft:mono:bold:italic:size=8:antialias=false
! Scroll
urxvt*scrollBar: false
! Transparency
!urxvt*transparent: true
!urxvt*shading: 30
urxvt*depth: 32
urxvt*background: [80]#000000
! Settings
urxvt*internalBorder: 3
URxvt*cursorUnderline: true
URxvt*cursorBlink: true
URxvt*savelines: 10000
! Optionnal settings
URxvt.letterSpace:-1
! Perl extensions
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.colorBD: white
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:copy_clipboard
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard,matcher
URxvt*perl-ext:
URxvt*perl-ext-common:
! Colorscheme
*foreground: #E5D2A5
!*background: #181512
! Black Black !
*color0: #252525
*color8: #454545
! Red Red !
*color1: #CB4F29
*color9: #CB4F29
! Green Green !
*color2: #A2B272
*color10: #B9C88E
! Yellow Yellow!
